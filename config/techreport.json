    "cwv_subcategories": [
      "fid",
      "cls",
      "lcp",
      "fcp",
      "inp",
      "ttfb"
    ],
    "lighthouse_subcategories": [
      "accessibility",
      "best_practices",
      "performance",
      "pwa",
      "seo"
    ]
  },
  "labels": {
    "values": {
      "date": "Date",
      "client": "Client",
      "origins": "Origins",

      "origins_eligible_for_cwv": "Eligible",
