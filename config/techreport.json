      "pct_good_lcp": "% Good LCP",
      "pct_good_fcp": "% Good FCP",
      "pct_good_inp": "% Good INP",
      "pct_good_ttfb": "% Good TTFB",

      "median_lighthouse_score_accessibility": "Median accessibility",
